{
  "expo": {
    "name": "Neates",
    "slug": "neates",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFC61E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
